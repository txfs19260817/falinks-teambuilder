    "downshift": "^6.1.12",
    "gray-matter": "^4.0.3",
    "i18next": "^22.4.9",
    "idb": "^7.1.1",
    "next": "^13.1.5",
    "next-i18next": "^13.0.3",
    "next-pwa": "^5.6.0",
    "next-seo": "^5.15.0",
    "nextjs-progressbar": "^0.0.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.41.5",
    "react-hot-toast": "^2.4.0",
