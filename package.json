    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.11.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "googleapis": "^118.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "next-sitemap": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prisma": "^4.15.0",
    "rimraf": "^5.0.0",
    "tailwind-scrollbar": "^3.0.4",
