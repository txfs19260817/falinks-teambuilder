    "@playwright/test": "^1.29.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/xlsx": "^0.0.36",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitest/coverage-c8": "^0.27.0",
    "autoprefixer": "^10.4.13",
