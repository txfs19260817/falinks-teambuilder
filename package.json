{
  "name": "falinks-teambuilder",
  "version": "2.0.0",
  "description": "A Pok√©mon teambuilder allows multiple users to work on the same team collaboratively",
  "scripts": {
    "build": "next build",
    "build-prod-local": "run-s clean build start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "build-types": "tsc --noEmit --pretty --skipLibCheck",
    "clean": "rimraf .next out coverage test-results",
    "cronjob": "run-s cronjob:*",
    "db-generate": "prisma generate",
    "db-migrate": "prisma migrate deploy",
    "db-migrate-dev": "prisma migrate dev",
    "db-reset": "prisma migrate reset",
    "db-seed": "prisma db seed",
    "cronjob:syncvgc": "ts-node -r tsconfig-paths/register -O {\\\"module\\\":\\\"commonjs\\\",\\\"paths\\\":{\\\"@/*\\\":[\\\"./src/*\\\"]},\\\"baseUrl\\\":\\\".\\\"} scripts/sync_vgcpastes.ts",
    "db-prisma-studio": "prisma studio",
    "db-pull": "prisma db pull",
    "dev": "run-p dev:next dev:ws",
    "dev:next": "next dev",
    "dev:ws": "npx y-websocket@1.4.5 --port 1234",
    "env-pull": "vercel env pull",
    "env-pull-dev": "vercel env pull .env.development.local",
    "export": "next export",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "postinstall": "run-s db-generate",
    "prepare": "husky install",
    "start": "next start",
    "pretest": "npx playwright install --with-deps",
    "test": "run-p test:e2e test:unit",
    "test:e2e": "playwright test",
    "test:unit": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
