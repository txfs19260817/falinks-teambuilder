    "@tiptap/extension-placeholder": "^2.0.3",
    "@tiptap/react": "^2.0.3",
    "@tiptap/starter-kit": "^2.0.3",
    "@vercel/analytics": "^1.0.1",
    "daisyui": "^2.51.6",
    "downshift": "^6.1.12",
    "gray-matter": "^4.0.3",
    "i18next": "^22.5.1",
    "idb": "^7.1.1",
    "next": "^13.4.1",
    "next-i18next": "^13.3.0",
    "next-pwa": "^5.6.0",
    "next-seo": "^6.0.0",
    "nextjs-progressbar": "^0.0.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^12.3.1",
    "remark": "^14.0.3",
    "remark-html": "^15.0.2",
    "smogon": "^2.2.9",
    "swr": "^2.1.5",
    "theme-change": "^2.5.0",
